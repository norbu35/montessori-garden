{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "types": [
      "@astrojs/image/client"
    ],
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ]
    }
  }
}
